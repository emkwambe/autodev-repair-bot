# AutoDev Repair Bot - Environment Configuration
# Copy this file to .env and fill in your values

# Required: OpenAI API Key for GPT-4o reasoning
OPENAI_API_KEY=sk-xxxx

# Required: GitHub Personal Access Token for PR creation
# Needs: repo, workflow permissions
GITHUB_TOKEN=ghp_xxxx

# Optional: Default test command
AUTODEV_TEST_COMMAND=pytest -q

# Optional: Maximum repair attempts (default: 2)
AUTODEV_MAX_ATTEMPTS=2

# Optional: Enable debug logging
AUTODEV_DEBUG=false
